{
    "name": "dapasa/laravel-env-set-command",
    "description": "Set a .env file variable from the command line",
    "keywords": [
        "dapasa",
        "env-set",
        "environment",
        "variables",
        "env",
        "artisan"
    ],
    "homepage": "https://github.com/dapasa/env-set",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Passrelli",
            "email": "dpassarelli@dpassarelli.com",
            "homepage": "https://dpassarelli.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/console": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0|^9.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "dapasa\\EnvironmentSetCommand\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "dapasa\\EnvironmentSetCommand\\EnvironmentSetCommandServiceProvider"
            ]
        }
    }
}
